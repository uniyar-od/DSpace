        <jsScript_script>var workflow = 'defaultWorkflow';
var previousActionRequiresUI = 'true';
var workflowStepNew = 'reviewstep';
var previousWorkflowStepNew = 'reviewstep';
var previousWorkflowAction = 'claimaction';
var workflowAction = 'reviewaction';

var actorNew = eperson_uuid;

if(workflowStep == 'SUBMIT'){
	workflowStepNew = 'reviewstep';
	workflowAction = 'claimaction';
	previousWorkflowStepNew = 'submit';
	previousWorkflowAction = 'submit';
    actorNew = null;
} else if(workflowStep == 'STEP1POOL'){
	workflowStepNew = 'reviewstep';
	workflowAction = 'reviewaction';
	previousWorkflowStepNew = 'reviewstep';
	previousWorkflowAction = 'claimaction';
} else if(workflowStep == 'STEP1'){
	workflowStepNew = 'editstep';
	workflowAction = 'claimaction';
	previousWorkflowStepNew = 'reviewstep';
	previousWorkflowAction = 'reviewaction';
} else if(workflowStep == 'STEP2POOL'){
	workflowStepNew = 'editstep';
	workflowAction = 'editaction';
	previousWorkflowStepNew = 'editstep';
	previousWorkflowAction = 'claimaction';
} else if(workflowStep == 'STEP2'){
	workflowStepNew = 'finaleditstep';
	workflowAction = 'claimaction';
	previousWorkflowStepNew = 'editstep';
	previousWorkflowAction = 'editaction';
} else if(workflowStep == 'STEP3POOL'){
	workflowStepNew = 'finaleditstep';
	workflowAction = 'finaleditaction';
	previousWorkflowStepNew = 'finaleditstep';
	previousWorkflowAction = 'claimaction';
} else if(workflowStep == 'STEP3'){
	workflowStepNew = 'item';
	workflowAction = 'approve';
	previousWorkflowStepNew = 'finaleditstep';
	previousWorkflowAction = 'finaleditaction';
}</jsScript_script>
