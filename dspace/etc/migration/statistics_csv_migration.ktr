    <connection>dspace</connection>
    <sql>SELECT item_id as dso_id, '2' as entity_type, uuid as dso_uuid
FROM item
where item_id is not null

UNION

select entity.id as dso_id, entity.type as entity_type, metadata_value.dspace_object_id as dso_uuid
FROM (
   select id, crisid, '9' as type FROM old_cris_rpage
   UNION select id, crisid, '11' as type  FROM old_cris_orgunit
   UNION select id, crisid, '1000' as type  FROM old_cris_do
   UNION select id, crisid, '10' as type  FROM old_cris_project
) as entity, 
metadatavalue metadata_value, 
metadatafieldregistry metadata_field, 
metadataschemaregistry metadata_schema
WHERE metadata_value.metadata_field_id = metadata_field.metadata_field_id
AND metadata_field.element = 'legacyId'
AND metadata_field.qualifier is null
AND metadata_field.metadata_schema_id = metadata_schema.metadata_schema_id
AND metadata_schema.short_id = 'cris'
AND metadata_value.text_value = entity.crisid

UNION

SELECT bitstream_id as dso_id, '0' as entity_type, uuid as dso_uuid
FROM bitstream
where bitstream_id is not null

UNION

SELECT eperson_id as dso_id, '7' as entity_type, uuid as dso_uuid
FROM eperson
where eperson_id is not null

UNION

SELECT collection_id as dso_id, '3' as entity_type, uuid as dso_uuid
FROM collection
where collection_id is not null

UNION
SELECT community_id as dso_id, '4' as entity_type, uuid as dso_uuid
FROM community
where community_id is not null

</sql>
