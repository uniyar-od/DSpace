        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>rights</dc-element>
          <dc-qualifier>uri</dc-qualifier>
          <parent />
          <label>License</label>
          <input-type value-pairs-name="common_licenses">dropdown</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>If you publish metadata with fulltext, please choose a license for your work. You can also choose to grant no license. Examples see https://creativecommons.org/licenses/?lang=en</hint>
        </field>
