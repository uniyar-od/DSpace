          <dc-element>rights</dc-element>
          <dc-qualifier />
          <parent />
          <label>License Version</label>
          <input-type value-pairs-name="version">dropdown</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>If you publish metadata with fulltext, please choose a license version for your work. You can also choose to grant no license. Examples see https://creativecommons.org/licenses/?lang=en</hint>
        </field>
-->    
    <field>
          <dc-schema>dc</dc-schema>
          <dc-element>identifier</dc-element>
          <dc-qualifier />
          <parent />
          <label>Identifier(s) (e.g. DOI, ISBN, ISSN)</label>
          <input-type value-pairs-name="common_identifiers">qualdrop_value</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>If your publication has a Persistent Identifier (e.g. DOI, ISBN, ISSN, Scopus eID), please include it.</hint>
        </field>
<!-- auskommentiert, da DOI als Link in dc.identifier.doi; A.G., 16.01.2020  -->
<!--
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>doi</dc-qualifier>
          <parent />
          <label>Publisher DOI</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the DOI provided by the original publisher. Example DOI: https://doi.org/10.20366/unimib/unidata/sn196-1.0</hint>
        </field>
-->    
    <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>articlenumber</dc-qualifier>
          <parent />
          <label>Article ID</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the article ID or Book Part's ID. Example: Volume 2015, Article ID 212910, 10 pages</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>publisher</dc-element>
          <dc-qualifier />
          <parent />
          <label>Publisher</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the name of the publisher of the peviously issued instance of this item.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>contributor</dc-element>
          <dc-qualifier>advisor</dc-qualifier>
          <parent />
          <label>Advisor(s)</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the name(s) of the advisor(s).</hint>
          <type-bind>Thesis</type-bind>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>contributor</dc-element>
          <dc-qualifier>referee</dc-qualifier>
          <parent />
          <label>Referee(s)</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the name(s) of the referee(s).</hint>
          <type-bind>Thesis</type-bind>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>contributor</dc-element>
          <dc-qualifier>grantor</dc-qualifier>
          <parent />
          <label>Granting Institution</label>
          <input-type value-pairs-name="granting_organisation">dropdown</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Choose the name of the granting institution.</hint>
          <type-bind>Thesis</type-bind>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>type</dc-element>
          <dc-qualifier>thesis</dc-qualifier>
          <parent />
          <label>Type of Work</label>
          <input-type value-pairs-name="type_thesis">dropdown</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Please choose the type of thesis.</hint>
          <type-bind>Thesis</type-bind>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>dateAccepted</dc-element>
          <dc-qualifier />
          <parent />
          <label>Date of final exam</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the date of the final exam.</hint>
          <type-bind>Thesis</type-bind>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>identifier</dc-element>
          <dc-qualifier>citation</dc-qualifier>
          <parent />
          <label>Citation</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the standard citation for the previously issued instance of this item, e.g. "Catalysts 2016, 6(10), 160")</hint>
        </field>
      </page>
      <page number="3" heading="3">
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>subject</dc-element>
          <dc-qualifier>other</dc-qualifier>
          <parent />
          <label>Subject Keywords</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Please enter up to five keywords or phrases in German as well as in English. Please enter separate keywords by using "Add More". Please use the singular.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>conference</dc-qualifier>
          <parent />
          <label>Conference</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the name of the conference where the item has been presented, if any. Examples see https://openhsu.ub.hsu-hh.de/cris/events/events00107. If your conference is listed below, please use the database entries.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier />
          <parent />
          <label>Project</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the name of the sponsor or funder, and/or project/grant name or ID: Please use the following syntax if applicable: EC/FP7/12345/ProjectName/ProjectAcronym, like "Kr 752/33-1". If your project is listed below, please use the database entries.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>ispartof</dc-qualifier>
          <parent />
          <label>Series or Journal</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>If a document appeared in a publication series, then the name of the series are given here.</hint>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>volume</dc-qualifier>
          <parent />
          <label>Periodical volume</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the volume of the journal.</hint>
        </field>
<!-- zu Testzwecken bzgl. BibTex-Import eingebunden; A.G., 16.01.2020  -->
 <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>volume</dc-qualifier>
          <parent />
          <label>Periodical volume (Test)</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the volume of the journal.</hint>
        </field>

        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>issue</dc-qualifier>
          <parent />
          <label>Periodical issue</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the issue of the journal.</hint>
        </field>
<!-- zu Testzwecken für BibTex-Import, A.G., 16.01.2020  -->
zu Testzwecken für BibTex-Import, A.G., 16.01.2020
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>issue</dc-qualifier>
          <parent />
          <label>Issue (Test)</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the issue of the journal.</hint>
        </field>
       <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>pagestart</dc-qualifier>
          <parent />
          <label>First page</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the article's or Book Part's first page.</hint>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>pageend</dc-qualifier>
          <parent />
          <label>Last page</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the article's or Book Part's last page.</hint>
        </field>
<!-- für Testzwecke für BibTex-Import, A.G., 16.01.2020  -->
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>pages</dc-qualifier>
          <parent />
          <label>Pages (from...to) (Test)</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the article's or Book Part's pages (from...to).</hint>
        </field>

        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>booktitle</dc-qualifier>
          <parent />
          <label>Book title</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>You have to enter the title of the book, if your publication is a chapter of the book.</hint>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>number</dc-qualifier>
          <parent />
          <label>Volume (part of multivolume book)</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the volume number of the book (if the book consists of several volumes).</hint>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>originalpublishername</dc-qualifier>
          <parent />
          <label>Name of the original publisher</label>
          <input-type>onebox</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Enter the name of the original publisher.</hint>
        </field>
        <field>
          <dc-schema>dcterms</dc-schema>
          <dc-element>bibliographicCitation</dc-element>
          <dc-qualifier>originalpublisherplace</dc-qualifier>
          <parent />
          <label>Place of original publisher</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the place of the original publisher.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>description</dc-element>
          <dc-qualifier>edition</dc-qualifier>
          <parent />
          <label>Edition</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Enter the edition of the book (leave empty for 1st edition).</hint>
        </field>
<!-- auskommentiert; A.G., 30.12.2019
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>issupplementedby</dc-qualifier>
          <parent />
          <label>Is Supplemented By</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint />
        </field>
-->
<!-- auskommentiert; A.G., 30.12.2019
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>issupplementto</dc-qualifier>
          <parent />
          <label>Is Supplement To</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint />
        </field>
-->
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>references</dc-qualifier>
          <parent />
          <label>References</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>A related resource that is referenced, cited, or otherwise pointed to by the described resource.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>isreferencedby</dc-qualifier>
          <parent />
          <label>Is Referenced By</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Pointed to by referenced resource. A related resource that references, cites, or otherwise points to the described resource.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>haspart</dc-qualifier>
          <parent />
          <label>Has Part</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>References physically or logically contained item, e.g. contributions in a conference proceeding).</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>isformatof</dc-qualifier>
          <parent />
          <label>Is Format Of</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>References additional physical form. A related resource that is substantially the same as the described resource, but in another format.</hint>
        </field>
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>isversionof</dc-qualifier>
          <parent />
          <label>Is Version Of</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>References earlier or later version. A related resource of which the described resource is a version, edition, or adaptation.</hint>
        </field>
        

        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>type</dc-element>
          <dc-qualifier>version</dc-qualifier>
          <parent />
          <label>Version</label>
          <input-type value-pairs-name="type_versions">dropdown</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Choose the type of version. For type "Article", "Book Part" and "Book" choose either "Accepted Version" for postprints, "Published Version" for the publisher PDF or "Updated Version" for any version of the article,</hint>
        </field>

<!-- für Freitextfeld; A.G., 30.12.2019 -->
        <field>
          <dc-schema>dc</dc-schema>
          <dc-element>relation</dc-element>
          <dc-qualifier>hasversion</dc-qualifier>
          <parent />
          <label>Free text</label>
          <input-type>onebox</input-type>
          <repeatable>true</repeatable>
          <required />
          <hint>Your comments or suggestions</hint>
        </field>

        <field>
          <dc-schema>hsu</dc-schema>
          <dc-element>accessrights</dc-element>
          <dc-qualifier>dnb</dc-qualifier>
          <parent />
          <label>Access rights DNB</label>
          <input-type value-pairs-name="accessrights_dnb">dropdown</input-type>
          <repeatable>false</repeatable>
          <required />
          <hint>Choose which rights the German National Library (DNB) has to display archived items.</hint>
          <visibility>workflow</visibility>
        </field>
      </page>
    </form>
  </form-definitions>
  <form-value-pairs>
    <value-pairs value-pairs-name="common_iso_languages" dc-term="common_iso_languages">
      <pair>
        <displayed-value>English (United States)</displayed-value>
        <stored-value>en_US</stored-value>
      </pair>
      <pair>
        <displayed-value>English</displayed-value>
        <stored-value>en</stored-value>
      </pair>
      <pair>
        <displayed-value>Spanish</displayed-value>
        <stored-value>es</stored-value>
      </pair>
      <pair>
        <displayed-value>German</displayed-value>
        <stored-value>de</stored-value>
      </pair>
      <pair>
        <displayed-value>French</displayed-value>
        <stored-value>fr</stored-value>
      </pair>
