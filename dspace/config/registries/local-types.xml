<?xml version="1.0"?>
<!--
  - local-types.xml
  -
  - An empty registry for instance-specific, local metadata. Any non-standard
  - metadata fields should be added here to avoid polluting the standard
  - namespaces.
  - 
  -->
<dspace-dc-types>

  <dspace-header>
    <title>Registry of instance-specific metadata fields</title>
    <contributor.author>Ivan Mas&#xE1;r</contributor.author>
    <contributor.editor>$Author$</contributor.editor>
    <date.created>$Date$</date.created>
    <description>$Log$</description>
    <description.version>$Revision$</description.version>
  </dspace-header>

  <dc-schema>
    <name>local</name>
    <namespace>http://dspace.org/namespace/local/</namespace>
  </dc-schema>

<dc-type>
    <schema>local</schema>
    <element>message</element>
    <qualifier>claim</qualifier>
    <scope_note></scope_note>
  </dc-type>

<!--
  <dc-type>
    <schema>local</schema>
    <element></element>
    <qualifier></qualifier>
    <scope_note>Description of example metadata field.</scope_note>
  </dc-type>
-->

  <dc-type>
    <schema>local</schema>
    <element>message</element>
    <qualifier>claim</qualifier>
    <scope_note>Enable the user to add notes about his claiming request</scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>fakeitem</element>
    <scope_note>Set to true/yes/on to flag the item for deletation upon
	deposit by the DeleteFakeItem consumer</scope_note>
  </dc-type>

  <dc-type>
    <schema>local</schema>
    <element>patent</element>
    <qualifier>type</qualifier>
    <scope_note>specific type for patents</scope_note>
  </dc-type>

  <dc-type>
    <schema>dc</schema>
    <element>subject</element>
    <qualifier>keywordsplus</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>dc</schema>
    <element>subject</element>
    <qualifier>meshsecondary</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>dc</schema>
    <element>subject</element>
    <qualifier>meshterms</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>dc</schema>
    <element>subject</element>
    <qualifier>meshtermssecondary</qualifier>
    <scope_note></scope_note>
  </dc-type>
</dspace-dc-types>
