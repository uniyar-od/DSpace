        <property name="viewMode" value="chart.pie"/>
        <property name="maxResults" value="10"/>
        <property name="categoryQueries">
            <map>
                <entry key="article" value="itemtype_keyword: ( 'research article' OR 'journal article' )"/>
                <entry key="book" value="itemtype_keyword: 'book'"/>
                <entry key="bookChapter" value="itemtype_keyword: 'book part'"/>
                <entry key="dataset" value="itemtype_keyword: ( 'dataset' OR 'interview' ) "/>
                <entry key="thesis" value="itemtype_keyword: ( 'thesis' OR 'bachelor thesis' OR 'doctoral thesis' OR 'master thesis' )"/>
                <entry key="other" value="OTHER"/>
            </map>
        </property>
    </bean>

